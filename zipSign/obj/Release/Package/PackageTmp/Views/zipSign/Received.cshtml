</style>
<script>

    var columnDefs = [
        { headerName: '', field: '', width: 40, checkboxSelection: true },
        { headerName: 'Sr No.', field: 'srNo', width: 80, suppressMovable: true, sortable: true, },
        { headerName: 'File Name', field: 'dname', width: 250, suppressMovable: true, sortable: true, },
        { headerName: 'Document Name', field: 'aname', width: 250, suppressMovable: true, sortable: true, },
        { headerName: 'Status', field: 'stat', width: 200, suppressMovable: true, sortable: true, },
        //{ headerName: 'Document Uploaded On', field: 'don', width: 200, suppressMovable: true, sortable: true, },
        //{ headerName: 'Document Uploaded By', field: 'dby', width: 200, suppressMovable: true, sortable: true, },
        //{ headerName: 'Document Received On', field: 'ron', width: 200, suppressMovable: true, sortable: true, },
        //{ headerName: 'Document Signed On', field: 'son', width: 200, suppressMovable: true, sortable: true, },
        {
            headerName: 'Action', field: '', width: 100, sortable: true, resizable: false, suppressMovable: true, cellRenderer: function (params) {
                return '<button type="button" class="ingridbtn" data-bs-toggle="modal" data-bs-target="#usermodal">View More</button>'
            }
        },
        {
            headerName: '', field: '', width: 50, sortable: true, resizable: false, suppressMovable: true, cellRenderer: function (params) {
                return '<span class="fa fa-trash gridIcon" id=""></span>'
            }
        },
    ];


    var rowData = [
        { srNo: '1', dcode: '4a56dfs6a', desc: '', stat: '' },




    ];

    var gridOptions = {
        columnDefs: columnDefs,
        rowData: rowData,
        pagination: false
    };

    document.addEventListener('DOMContentLoaded', function () {
        var gridDiv = document.querySelector('#myGrid');
        new agGrid.Grid(gridDiv, gridOptions);
    });
</script>

