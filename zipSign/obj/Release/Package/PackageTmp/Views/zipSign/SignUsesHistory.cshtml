@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<script>

    var columnDefs = [
        { headerName: 'Sr No.', field: 'srNo', width: 80, resizable: false, sortable: true, suppressMovable: true },
        { headerName: 'Sign Type', field: 'stype', width: 180, resizable: false, sortable: true, suppressMovable: true  },
        { headerName: 'Document Name', field: 'dname', width: 250, resizable: false, sortable: true, suppressMovable: true  },
        { headerName: 'Sign Date', field: 'sDate', width: 510, resizable: false, sortable: true, suppressMovable: true  },
    ];


    var rowData = [
        { srNo: '1', stype: 'eSign Aadhaar', dname: 'document 1', sDate: '15/06/2023' },
        { srNo: '2', stype: 'eSign Aadhaar', dname: 'document 2', sDate: '16/06/2023' },



    ];

    var gridOptions = {
        columnDefs: columnDefs,
        rowData: rowData,
        pagination: false
    };

    document.addEventListener('DOMContentLoaded', function () {
        var gridDiv = document.querySelector('#myGrid');
        new agGrid.Grid(gridDiv, gridOptions);
    });
</script>

